@model HotelBooking

<h1>Hotel Booking Details</h1>
<hr />

<div class="row">
    <div class="col-6">
        <div class="card">
            <div class="card-header">

            </div>

            <div class="card-body">
                <p>@Model.Id</p>
                <p>Guest Name: @Model.GuestName</p>
                <p>Check In Date: @Model.CheckIn</p>
                <p>Check Out Date: @Model.CheckOut</p>
            </div>

            <div class="card-footer">

            </div>
        </div>
    </div>
</div>